<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>MatchServlet</servlet-name>
        <servlet-class>MatchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GroupServlet</servlet-name>
        <servlet-class>GroupServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValueServlet</servlet-name>
        <servlet-class>ValueServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConversationServlet</servlet-name>
        <servlet-class>ConversationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListingsServlet</servlet-name>
        <servlet-class>Listings.ListingsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MatchServlet</servlet-name>
        <url-pattern>/MatchServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GroupServlet</servlet-name>
        <url-pattern>/GroupServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValueServlet</servlet-name>
        <url-pattern>/ValueServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConversationServlet</servlet-name>
        <url-pattern>/ConversationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListingsServlet</servlet-name>
        <url-pattern>/ListingsServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
