<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>TableServlet</servlet-name>
        <servlet-class>TableServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NextServlet</servlet-name>
        <servlet-class>NextServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchServlet</servlet-name>
        <servlet-class>SearchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RowAdd</servlet-name>
        <servlet-class>RowAdd</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>XMLServlet</servlet-name>
        <servlet-class>XMLServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>XMLServler2</servlet-name>
        <servlet-class>XMLServler2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ignore</servlet-name>
        <servlet-class>ignore</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TableServlet</servlet-name>
        <url-pattern>/TableServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>NewServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NextServlet</servlet-name>
        <url-pattern>/NextServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchServlet</servlet-name>
        <url-pattern>/SearchServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RowAdd</servlet-name>
        <url-pattern>/RowAdd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>XMLServlet</servlet-name>
        <url-pattern>/XMLServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>XMLServler2</servlet-name>
        <url-pattern>/XMLServler2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ignore</servlet-name>
        <url-pattern>/ignore</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
